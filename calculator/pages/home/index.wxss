.wrapper{
  padding: 30rpx 38rpx;
}
.header{
  background: #FFFFFF;
  text-align: center;
  padding: 25rpx 0;
}
.icon-dizhi1{
  color: #FE3666;
}
.yourcity{
  font-size: 32rpx;
  font-weight: bold;
  color: #222222;
}
.city{
  font-size: 28rpx;
  margin-top: 20rpx;
  color: #222222;
}
.content{
  background: #FFFFFF;
  margin-top: 20rpx;
  padding: 40rpx 28rpx;
  color: #222222;
}
.content-title{
  font-size: 28rpx;
  font-weight: bold;
  text-align: center;
  position: relative;
}
.content-con{
  margin-top: 30rpx;
}
.item-content{
  display: flex;
  justify-content: space-between;
  margin-top: 20rpx;
}
.content-item{
  padding-bottom: 32rpx;
}
.nopadding{
  padding: 0;
}
.size{
  padding:0 30rpx;
  line-height: 60rpx;
  border: 1rpx solid #cecece;
  border-radius: 100rpx;
  font-size: 24rpx;
}
.size input{
  width: 100%;
  height: 100%;
}
.input{
  width: 80rpx;
}

.footer{
  margin-top: 60rpx;
}
.start{
  background: #09AF17;
  border-radius: 6rpx;
  font-size: 32rpx;
  color: #FFFFFF;
  line-height: 88rpx;
  width: 80%;
}
.content-title-bg{
  width:450rpx;
  position: absolute;
  z-index: 1;
  left: 50%;
  transform: translateX(-50%);
}
.title-text{
  position: relative;
  z-index: 2;
  color: #FFFFFF;
  line-height: 60rpx;
}
.item-label{
  color: #222222;
  font-size: 28rpx;
  font-weight: bold;
}
.self-input{
  background: #D8D8D8;
  color: #888888;
  font-size: 24rpx;
  max-width: 150rpx;
  overflow: hidden;
  max-height: 60rpx;
}
.active{
  border-color: transparent;
  color: #FFFFFF;
  background-image: linear-gradient(-138deg, #F72386 0%, #FA6C00 100%);
}