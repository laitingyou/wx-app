page {
    color: #4A4A4A;
}

.image {
    background-color: #eee;
    width: 100%;
    height: 100%;
}
.myPage{
    overflow-x: hidden;
}
.header{
    position: relative;
    max-height: 300rpx;
    overflow: hidden;
}
/*-----直播inlive-----*/
.live{
    margin-top: 20rpx;
    width: 100%;
    background-color: #fff;

}
.live-header{
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 20rpx 22rpx 0;

}
.live-header-left{
    display: flex;
    align-items: center;
    justify-content: space-between;
    font-size: 32rpx;
    color: #222222;
    letter-spacing: 0;
    line-height: 32rpx;
    width: 180rpx;
}
.live-header-left-icon{
    width: 40rpx;
    height: 40rpx;
}
.live-icon::before{
    content: '\e6c3';
    color: #F9007C;
    font-size: 40rpx;
}


/****直播区域****/
.liveArea {
    position: relative;
    margin-top: -20rpx;
    width: 100%;
    /*min-height: 400rpx;*/
    box-sizing: border-box;

}
.liveArea-header{
}
.today-live{
    position: relative;
    z-index: 1;
    background: #FFFFFF;
}
.onePending-margin{
    margin-top: 20rpx;
}

.liveArea-header-bgImg {
    position: absolute;
    z-index: 0;
    right: 0;
    left: 50%;
    transform: translateX(-50%);
    width: 80%;
    top: 10rpx;

}
/*标题*/
.liveArea-title {
    flex-wrap: wrap;
    padding: 30rpx 28rpx;
    color: rgba(254, 54, 102, 0.82);
    font-size: 36rpx;
    font-weight: bold;
    text-align: center;
}
.liveArea-title-icon {
    flex: 0 0 50rpx;
    font-size: 40rpx;
}
.liveArea-title-h1,.goods-title {
    flex: 0 0 auto;
    text-indent: 10rpx;
}
.liveArea .liveArea-title{
}
.liveArea-title-h2,.goods-little-title {
    flex: 1 1 auto;
    width: 0;
    padding: 0 20rpx;
}

.onePending {
    background: #ffffff;
}
.onePending-list{
    padding:0 24rpx 24rpx;
}
.onePendingTitle  {
    position: relative;
    display: block;
    font-size: 32rpx;
    color: #00B0F0;
    text-align: center;
    height: 60rpx;
    line-height: 60rpx;
    padding: 20rpx 0;
    margin: 0 -24rpx;
    background-color: #f1f1f1;
}
.onePendingTitle-img {
    position: absolute;
    z-index: 0;
    top: 30rpx;
    left: 50%;
    margin-left: -150rpx;
    width: 300rpx;
}

.advanceList {
    background-color: #f1f1f1;
}
.icon-zhibo1:before{
    color: #ff0064;
}
.icon-pintuan{
    color: #00B0F0;
}
.icon-service{
    color: #fe9d1b;
}
.icon-dizhi1{
    color: #00B0F0;
}


/*------广告--------*/
.advertImg{
    width: 100%;
    height: 100%;
}
.nav{
    display: flex;
}
.w-100{
    width: 100%;
}
.text-align{
    text-align: center;
}
.nav-item{
    color: #FFFFFF;
    line-height: 88rpx;
}
.currentNav{
    background-image: linear-gradient(-138deg, #F72386 0%, #FA6C00 100%);
    position: relative;
}
.nav-item:after{
    content: "";
    display: block;
    width: 0rpx;
    height: 0rpx;
    left: 45%;
    position: absolute;
    border-right:16rpx solid  rgba(0,0,0,0);
    border-bottom:16rpx solid rgba(0,0,0,0);
    border-left:16rpx solid  rgba(0,0,0,0);
}
.currentNav-active:after{
    border-top:16rpx solid #F72386;
}
.nextNav-active:after{
    border-top:16rpx solid #09AF17;
}
.nextNav{
    background: #09AF17;
    position: relative;
}

.msg{
    display: flex;
    justify-content: left;
    align-items: center;
    padding:0rpx 28rpx;
    margin-top: -10rpx;
}
.msg-user{
    display: flex;
    justify-content: left;
    align-items: center;
    height: 100%;
}
.msg-user-box{
    overflow: hidden;
    height: 80rpx;
}
.avatar{
    width: 60rpx;
    height: 60rpx;
    border-radius: 50%;
    overflow: hidden;
    margin-right: 20rpx;
}
.msg-text{
    color: #4A4A4A;
    font-size: 24rpx;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    width: 85%;
}
.public{
    color: #999999;
    font-size: 40rpx;
    margin-right: 30rpx;
}
/*----item----*/
.item{
    display: flex;
    justify-content: space-between;
    width: 100%;
    box-sizing: border-box;
    padding: 20rpx 28rpx ;
    margin: 20rpx 0;
    position: relative;
    background: #FFFFFF;
}
.row-img{
    width: 268rpx;
    height: 268rpx;
    display: block;
    background-color: rgba(238, 238, 238, 0.58);
    border: 1rpx solid #e0e0e0;
    position: relative;
}
.row-content{
    position: relative;
    height: 268rpx;
    width: 60%;
    margin-left: 20rpx;
    box-sizing: border-box;
}
.row-title{
    width: 100%;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
    overflow: hidden;
    font-size: 28rpx;
    color: #4A4A4A;
    letter-spacing: 0.92rpx;
    line-height: 40rpx;
    margin-top: 10rpx;
}
.dingyue{
    background: #09AF17 !important;
}
.row-invitation{
    overflow: hidden;
    margin:10rpx 0 0 0;
    box-sizing: border-box;
    font-size: 28rpx;
    color: #888888;
    position: absolute;
    bottom: 70rpx;
    width: 100%;
}
.row-invitation-left{
    display: block;
}
.row-invitation-right{
    display: block;
}
.red{
    color: #FE3666;
}
.price{
    font-size: 28rpx;
    vertical-align: bottom;
}
.row-price{
    font-size: 50rpx;
    color:#f62e43;
    font-weight: bold;
}
.fuhao{
    font-weight: bold;
}
.row-bottom{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: flex-end;
    position: absolute;
    bottom: 0;
    width: 100%;

}
.row-amount{
    font-size: 24rpx;
    color: #888888;
    letter-spacing: 0.86rpx;
    line-height: 36rpx;

}
.row-btn{
    color: #ffffff;
    line-height: 52rpx;
    border-radius:40rpx;
    width: 156rpx;
    font-size: 28rpx;
    position: absolute;
    text-align: center;
    right: 0;
    background-image: linear-gradient(-138deg, #F72386 0%, #FA6C00 100%);
}
.row-btn-box{
    width: 156rpx;
    height: 80rpx;
}

.price{
    color: #f62e43;
}

/*rule*/
.rule{
    position: absolute;
    top: 20rpx;
    right: 0;
    z-index: 99;
    background: rgba(2, 2, 2, 0.38);
    color: #FFFFFF;
    bottom: 100rpx;
    font-size: 22rpx;
    padding: 10rpx 20rpx;
    border-top-left-radius: 50rpx;
    border-bottom-left-radius: 50rpx;
    height: 30rpx;
    line-height: 30rpx;
}

.rule-img{
    border-radius: 35rpx;
    width: 100%;
}
.soldout{
    background: #7a7a7a;
    width: 150rpx;
}
.share-float{
    background: #09AF17;
    color: #FFFFFF;
    position: fixed;
    right: 0;
    bottom: 100rpx;
    z-index: 99;
    font-size: 24rpx;
    padding: 10rpx 20rpx;
    border-top-left-radius: 50rpx;
    border-bottom-left-radius: 50rpx;
    opacity: .8;
    box-shadow: 1rpx 6rpx 10rpx 0rpx rgba(2, 2, 2, 0.38);
}