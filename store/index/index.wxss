/*店铺首页*/
.friends{background:#fff; padding:0 27rpx;}
.friends.pad20{padding-bottom:20rpx;}
.friends .title{display:flex; padding:32rpx 0rpx 16rpx; line-height: 28rpx;  }
.friends .title .left{font-size:28rpx;
    flex: 1;
    }
.friends .title .right{color:#586c94 ;
    flex: 1;text-align: right}
.friends .title .right .icon-right {
    vertical-align: middle;
    font-size: 24rpx;
}
.friends .title .iconRight{display:block;
    line-height: 28rpx;}
.friends .ul{height:200rpx; white-space:nowrap; padding-top:16rpx; }
.friends .li{position:relative; display:inline-block; margin:0 10rpx; width:200rpx; height:200rpx;}
.friends .li image{width:200rpx; height:200rpx;}
.friends .num{position:absolute; left:10rpx; bottom:10rpx; color:#fff;}
.friends .ul .text{margin:0 auto; padding:30rpx 0; color:#888; text-align:center; font-size:35rpx;}
.friends .ul .text2{padding:60rpx 0;}
.friends .ul .botton{width:180rpx; height:50rpx; text-align:center; margin:20rpx auto 0; font-size:28rpx; line-height:50rpx; border-radius:10rpx; color:#fff; background:#fd3666;}
.friends .ul .botton.remind{background: #888;}

.goodsList{background:#f5f5f5;}
.goodsList .options{display:flex; justify-content:space-around; position: relative; background:#fff;
    position: sticky;   top: 70px; z-index: 999}
.options.pad20{padding-top:20rpx; border-top:1px solid rgba(224,224,224,0.30);}
.options .li{width:100%; text-align:center; color:#888;}
.options .li .text{font-size:30rpx; }
.options .li .num{padding:10rpx 0 20rpx;}
.options .li.active .text{color:#000;}
.options .select-red{position:absolute; height:8rpx; width:110rpx; margin-left:75rpx; bottom:0rpx; background:#fd3666; transition:0.5s;}

.goods{display:flex; justify-content:space-between; flex-wrap:wrap; margin-top:34rpx; background:#fff;}
.goods.column{flex-direction:column;}
.goodspd{padding:0 27rpx 20rpx;}
.goods .li{width:337rpx; margin-top:38rpx;}
.goods .li .img{position:relative; overflow:hidden; height:465rpx;}
.goods .li .img .icon-play{
    position: absolute;
     color: #fff;
    font-size: 100rpx;
    top: 35%;
    left: 35%;

}
.goods .li image{width:337rpx; height:465rpx;}
.goods .li .img .time{position:absolute; right: 0; border-radius: 20px  0 0 20px;  padding:0 20rpx;  height:52rpx; line-height:52rpx; bottom:20px; color:#fff; box-sizing:border-box; white-space:nowrap; overflow:hidden; text-overflow:ellipsis; background:rgba(0,0,0,.6);}

.goods .msg .price{display:block; margin-top:20rpx; line-height:40rpx;}
.goods .msg em{display:block; width:40rpx; height:38rpx; background:url(data:image/png;base64,); background-size:100% auto;}
.goods .msg .time{color:#b2b2b2;}

/*店铺朋友圈*/
.friendsList .li{display:flex; justify-content:space-between; padding:30rpx 27rpx; background:#fff; border-bottom:1px solid #f1f1f1;}
.friendsList .time{width:80rpx; min-width:80rpx; font-size:36rpx; line-height:36rpx; text-align:right; margin-right:30rpx; /*font-weight:bold;*/}
.friendsList .goods-msg{flex:1;}
.friendsList .send-time{display:flex; justify-content:space-between; height:42rpx; line-height:45rpx;}
.friendsList .mini-logo{display:flex;}
.friendsList .mini-logo .like,.friendsList .mini-logo .cart{display:block; width:42rpx; height:38rpx; margin-left:10rpx; z-index:10;  background:url(data:image/png;base64,); background-size:100% auto;}
.friendsList .mini-logo .cart{background:url(data:image/png;base64,);  background-size:100% auto;}
.friendsList .mini-logo .like.on{background: url(data:image/png;base64,); background-size:100% auto;}

.goodsMsg{flex:1;}
.goodsMsg .title{margin-bottom:20rpx; margin-top:-4rpx; line-height:40rpx; word-break:break-all;}
.goodsMsg .title.five{display: -webkit-box; -webkit-line-clamp:5; -webkit-box-orient: vertical; overflow: hidden;}
.goodsMsg .more-text{color:#43aaf3; padding-bottom:20rpx;}
.goodsMsg .ul{display: flex; flex-wrap:wrap; margin-bottom:10rpx;}
.goodsMsg .img{position:relative; margin-right:10rpx; margin-bottom:8rpx; height:187rpx;
}
.goodsMsg .img image{width:187rpx; height:187rpx;}
.goodsMsg .img.one { height: 376rpx}
.goodsMsg .img.one image{width:376rpx; height:376rpx;}
.goodsMsg .img .price{position:absolute; width:100%; height:45rpx; line-height:45rpx; padding-right:10rpx; box-sizing: border-box; bottom:0; color:#fff; text-align:right; background:linear-gradient(rgba(0,0,0,0),rgba(0,0,0,0.8));}


.star{position:relative; margin-top:20rpx; padding:0 28rpx; background:#f1f1f1;}
.star .star-top{position:absolute; top:-15rpx; left:40rpx; width: 0; height: 0; border-left:15rpx solid transparent; border-right:15rpx solid transparent; border-bottom:20rpx solid #f1f1f1;}
.star .star-name,.star .reply{padding:24rpx 0; line-height:40rpx; overflow: hidden;}
.star .reply{padding-bottom:18rpx;}
.star .reply.three{margin-bottom:15rpx;}
.star .star-name,.star .reply span{color:#7182a4;}
.star .star-name.bottom{border-bottom:1px solid #dedede;}
.star .star-name em{display:inline-flex; width:28rpx; height:25rpx; margin-right:10rpx; background: url(data:image/png;base64,); background-size:100% auto;}
.star .more{padding-bottom:30rpx;}

.loading{height:70rpx; line-height:70rpx; font-size:36rpx; color:#b2b2b2;}
.loading::after{border: none;}

.nogoods{width:350rpx; height:350rpx; display:flex; justify-content:center; align-self:center; margin-top:100rpx; background:url(data:image/png;base64,) no-repeat; background-size:100% auto;}
.nogoods-text{padding-top:200rpx; color:#ccc;}

.none{display:none;}

