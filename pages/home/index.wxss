}
.liveHit {

  display: inline-block;
  color: #fff;
  line-height: 40rpx;
  font-size: 20rpx;
  padding:0 15rpx;
  border-radius: 20rpx;
  background-image: linear-gradient(138deg,#f72386 0,#ff2853 100%);
  display: -webkit-flex;
  display: flex;
  align-items: center;
  width: 80rpx;
}
.yugao{
  background-color: #1EB6FC;
  color: #fff;
  line-height: 40rpx;
  font-size: 20rpx;
  padding:0 15rpx;
  border-radius: 20rpx;
  display: -webkit-flex;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 60rpx;
}
.live-theme{
  position: absolute;
  bottom: 60rpx;
  width: 100%;
  text-align: center;
  font-size: 28rpx;
  color: #FFFFFF;
  letter-spacing: 0;
  line-height: 50rpx;
  text-shadow: 0 2rpx 4rpx rgba(0,0,0,0.50);
  z-index:99 ;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.live-info{
  position: absolute;
  bottom: 20rpx;
  width: 230rpx;
  display: flex;
  justify-content:left;
  align-items: center;
  border-radius: 20rpx;
  background-color: rgba(52, 52, 52, 0.62);
  left: 50%;
  transform: translateX(-50%);
}
.live-people{
  color: #ffffff;
  font-size: 20rpx;
  margin-left: 10rpx;
}
.liveHit:before {
  content: '';
  background-color: #fff;
  width: 8rpx;
  height: 8rpx;
  margin-right: 8rpx;
  border-radius: 8rpx;
  animation: liveIcon .8s linear .8s infinite alternate;
}

@keyframes liveIcon{
  form {
    opacity: 1;
  }
  to {
    opacity: 0;
  }
}
@keyframes liveXin0{
  0% {
    opacity: 1;
    transform: translate3d(0,0,0)
  }
  34% {
    transform: translate3d(-100%,-150%,0)
  }
  100% {
    opacity: 0;
    transform: translate3d(0,-800%,0)
  }
}
@keyframes liveXin1{
  0% {
    opacity: 1;
    transform: translate3d(0,0,0)
  }
  45% {
    transform: translate3d(50%,-150%,0)
  }
  100% {
    opacity: 0;
    transform: translate3d(0,-800%,0)
  }
}
.xin {
  width: 50rpx;
  height: 50rpx;
  font-size: 50rpx;
  position: absolute;
  z-index: 1;
  bottom: 5%;
  right: 5%;
  opacity: 0;
}
.xin0:before {
  content: "\e617";
  color: #ff2853;
}
.xin1:before {
  content: "\e617";
  color: #008ef3;
}
.xin2:before {
  content: "\e617";
  color: #ffd032;
}
.xin3:before {
  content: "\e617";
  color: #009120;
}
.xin0 {
  animation: liveXin0 2.5s linear 0s infinite normal;
}
.xin1 {
  animation: liveXin0 2.5s linear .5s infinite normal;
}
.xin2 {
  animation: liveXin1 2.5s linear 1s infinite normal;
}
.xin3 {
  animation: liveXin1 2s linear 1.5s infinite normal;
}
