.preferential{
  background-color: #f0f0f0;
  font-size: 28rpx;
  position: relative;

}
.signed{
  width: 300rpx;
  line-height: 88rpx;
  border-radius: 200rpx;
  text-align: center;
  color: #ffffff;
  background-image: linear-gradient(-180deg, #FF857B 2%, #FC003C 100%);
}
.bg{
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 0;
}
.title{
  position: relative;
  z-index: 1;
  display: flex;
  flex-direction: column;
  text-align: center;
  color: #FFFFFF;
  padding-top: 40rpx;
}
.big-title{
  font-size: 48rpx;
  font-weight: bold;
}
.little-title{
  font-size: 24rpx;
}
.arrow-box{
  background-color: #ffffff;
}
.arrow{
    width: 84rpx;
    height: 42rpx;
    margin:0 auto ;
    display: block;

}
.tip{
  position: relative;
  z-index: 2;
  text-align: center;
  font-size: 24rpx;
  color: #4A4A4A;
  padding: 30rpx 0;
}

.top{
  text-align: center;
  padding: 40rpx 28rpx;
  background-color: #ffffff;
}

.signed{
  color: #ffffff;
  border-radius: 15px;
  width: 364rpx;
  line-height: 80rpx;
  margin: 130rpx auto 0;
  text-align: center;
  box-shadow:0 0 10rpx 2rpx rgba(65, 65, 65, 0.53);
  background-image: linear-gradient(-138deg, #F72386 0%, #FA6C00 100%);
}
.coupon-container{
  background: #FFFFFF;
  padding:20rpx 28rpx;
}
.coupons-item{


}
.store-info{
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
}
.store-img{
    width: 60rpx;
    height: 60rpx;
  border-radius: 6rpx;
  box-shadow:0 0 2rpx 2rpx rgba(208, 208, 208, 0.53);
}
.store-enter{
  background: #FE3666;
  border-radius: 6rpx;
  width: 160rpx;
  line-height: 60rpx;
  text-align: center;
  color: #FFFFFF;
}
.store-name-box{
  display: flex;
  align-items: center;
}
.store-name{
  font-size: 28rpx;
  color: #4A4A4A;
  margin-left: 20rpx;
}
.store-coupons{
  margin: 22rpx 0;
  transition: height 1s;
  overflow: hidden;
}
.puck-up{
  height: 660rpx;
}
.reset{
  height: 660rpx !important;
}
.coupons-item{
  width: 100%;
  background: #FCF2F5;
  border-radius: 6rpx;
  display: flex;
  justify-content: left;
  align-items: center;
  margin-bottom: 12rpx;
  position: relative;
  overflow: hidden;
}
.coupons-item::after{
  content: "";
  width: 30rpx;
  height: 30rpx;
  background: #FFFFFF;
  position: absolute;
  border-radius: 50%;
  transform: translateY(-50%);
  top: 0;
  right: 120rpx;
}
.coupons-item-img-box{
  width: 172rpx;
  height: 172rpx;
  margin: 20rpx 0 20rpx 20rpx;
  border-radius: 6rpx;
  overflow: hidden;
  box-shadow:0 0 2rpx 2rpx rgba(208, 208, 208, 0.53);
}
.coupons-item-img{
  width: 100%;
  height: 100%;
}
.coupons-item-info{
  margin-left: 30rpx;
  color: #888888;
}
.coupons-item-price{
  display: flex;
  justify-content: left;
  align-items: flex-end;
  color: #FF5F00;
}
.fuhao{
  font-size: 22rpx;
}
.price{
  font-size: 44rpx;
  font-weight: bold;
}
.price-tips{
font-size: 24rpx;
  margin-left: 20rpx;
}
.coupons-tips{
  font-size: 24rpx;
}

.coupons-item-date{
  font-size: 20rpx;
}
.coupon-many{
  text-align: center;
  font-size: 28rpx;
  color: #AAAAAA;
}
.icon-down{
  font-size: 28rpx;
}
.coupon-container{
  margin-bottom: 22rpx;
}
.time{
  background: #FF5D4B;
  border-radius: 6rpx;
  text-align: center;
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  color: #FFFFFF;
  font-size: 20rpx;
}
/*-----红包------*/
.redbag{
  position: fixed;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.7);
  z-index: 1006;
  opacity: 0;
  pointer-events: none;
}
.redbag-box{
  position: fixed;
  width: 666rpx;
  height: 904rpx;
  left: 50%;
  top: 0%;
  transform: translate(-50%,-100%);
  z-index: 1007;
  text-align: center;
  transition: all 100ms;
  opacity: 0;
  pointer-events: none;

}


.redbag-img{
  width: 100%;
  height: 100%;
}
.redbag-text{
  text-align: center;
  position: fixed;
  width: 80%;
  background:none;
  left: 50%;
  top: 44%;
  transform: translateX(-50%);
  z-index: 1008;
  height: 440rpx;
  line-height: inherit;

}
.redbag-title{
  font-size: 72rpx;
  color: #FFFF00;
  letter-spacing: 0;
  text-align: center;
  padding-left: 20rpx;
}

.redbag-content-top{
  font-size: 40rpx;
  color: #FFFF00;
  letter-spacing: 0;
  margin:40rpx 0 10rpx ;
  display: block;

}
.open{
  margin:20rpx 0 10rpx ;
}
.icon-cuo{
  color: #eeeeee;
  font-size: 30rpx;
  border: 1rpx solid #fff;
  padding: 20rpx;
  border-radius: 50%;
  position: absolute;
  bottom: -100rpx;
  left: 50%;
  width: 30rpx;
  height: 30rpx;
  transform: translateX(-50%);
}
.redbag-box-cuo{
  bottom: -30rpx;
}
.redbag-content-tc{
  font-size: 64rpx;
  color: #fff;
  letter-spacing: 0;
  line-height: 42rpx;
}
.redbag-content-open{
  color: #ffffff;
  letter-spacing: 0;
}
.redbag-content-bottom{
  font-size: 28rpx;
  color: #FFFF00;
  letter-spacing: 0;
  line-height: 28rpx;
}
.redbag-share-btn{
  position: fixed;
  width: 100%;
  height: 100rpx;
  left: 50%;
  bottom: 0rpx;
  transform: translateX(-50%);
  opacity: 0;
  z-index: 1010;
}
.btn-open{
  bottom: 50rpx;
  height: 200rpx;
  width: 150rpx;
}
.show{
  opacity: 1;
  pointer-events: all;

}
.bagShow{
  pointer-events: visible;
  top: 50%;
  opacity: 1;
  transform: translate(-50%,-50%);
}