.msgAnimation {
    animation-name: msgAnimation-transform-keyframes;
    animation-duration: 4000ms;
    animation-delay: 0ms;
    animation-fill-mode: forwards;
    animation-timing-function: linear;
    transform-origin: 0 0;
    opacity: 0;
}
@keyframes msgAnimation-transform-keyframes {
    0% {
        opacity: 1;transform:translate(0, 0) scale(1) rotateX(0deg) rotateY(0deg) rotateZ(0deg) translate(-50%, -50%);animation-timing-function: cubic-bezier(.25,.25,.75,.75);}
    33.33% {
        opacity: 1;transform:translate(50px, 0) scale(1) rotateX(0deg) rotateY(0deg) rotateZ(0deg) translate(-50%, -50%);animation-timing-function: cubic-bezier(.25,.25,.75,.75);}
    66.67% {
        opacity: 1;transform:translate(100px, 0) scale(1) rotateX(0deg) rotateY(0deg) rotateZ(0deg) translate(-50%, -50%);animation-timing-function: cubic-bezier(.25,.25,.75,.75);}
    88.67% {
        opacity: 1;transform:translate(120px, 0) scale(1) rotateX(0deg) rotateY(0deg) rotateZ(0deg) translate(-50%, -50%);animation-timing-function: cubic-bezier(.25,.25,.75,.75);}
    100% {
        opacity: 0;transform:translate(150px, 0) scale(1) rotateX(0deg) rotateY(0deg) rotateZ(0deg) translate(-50%, -50%);}
}

.msgAnimation2 {
    animation-name: msgAnimation-transform-keyframes2;
    animation-duration: 4000ms;
    animation-delay: 0ms;
    animation-fill-mode: forwards;
    animation-timing-function: linear;
    transform-origin: 0 0;
    opacity: 0;
}
@keyframes msgAnimation-transform-keyframes2 {
    0% {
        opacity: 1;transform:translate(0, 0) scale(1) rotateX(0deg) rotateY(0deg) rotateZ(0deg) translate(-50%, -50%);animation-timing-function: cubic-bezier(.25,.25,.75,.75);}
    33.33% {
        opacity: 1;transform:translate(50px, 0) scale(1) rotateX(0deg) rotateY(0deg) rotateZ(0deg) translate(-50%, -50%);animation-timing-function: cubic-bezier(.25,.25,.75,.75);}
    66.67% {
        opacity: 1;transform:translate(100px, 0) scale(1) rotateX(0deg) rotateY(0deg) rotateZ(0deg) translate(-50%, -50%);animation-timing-function: cubic-bezier(.25,.25,.75,.75);}
