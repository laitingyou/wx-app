.heartPraise {
    width: 60rpx;
    animation-name: heartPraise-transform-keyframes;
    animation-duration: 1500ms;
    animation-delay: 0ms;
    animation-fill-mode: forwards;
    animation-timing-function: linear;
    transform-origin: 0 0;
    position: absolute;
    z-index: 1;
    bottom: 28rpx;
    left: 28rpx;
    opacity: 0;
}
.heartPraise .icon-love{
    font-size: 60rpx;
}


@keyframes heartPraise-transform-keyframes {
    0% {
        opacity: 0;transform:translate(0px, 0px) scale(0) rotateX(0deg) rotateY(0deg) rotateZ(0deg) translate(-50%, -50%);animation-timing-function: cubic-bezier(.25,.25,.75,.75);}
    50% {
        opacity: 1;transform:translate(1px, -72px) scale(1) rotateX(0deg) rotateY(0deg) rotateZ(0deg) translate(-50%, -50%);animation-timing-function: cubic-bezier(.25,.25,.75,.75);}
    75% {
        opacity: 1;transform:translate(34px, -126px) scale(1) rotateX(0deg) rotateY(0deg) rotateZ(0deg) translate(-50%, -50%);animation-timing-function: cubic-bezier(.25,.25,.75,.75);}
    100% {
        opacity: 0;transform:translate(8px, -185px) scale(1) rotateX(0deg) rotateY(0deg) rotateZ(0deg) translate(-50%, -50%);}
}


