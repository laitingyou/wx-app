/* 授权按钮 */
.wxGetUserInfo {
    position: fixed;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 10;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, .2);
}
.wxGetUserInfo-Bj {
    background-color: rgba(0, 0, 0, .6);
    width: 100%;
    height: 100%;
}
.wxGetUserInfo-inner {
    background-color: #fff;
    border-radius: 8rpx;
    position: absolute;
    z-index: 1;
    top: 50%;
    left: 50%;
    box-sizing: border-box;
    width: 400rpx;
    height: 200rpx;
    margin-top: -100rpx;
    margin-left: -200rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}
/* 授权按钮 end */
.backApp {
    padding: 0 30rpx;
    height: 60rpx;
    line-height: 60rpx;
    border-radius: 30rpx;
    color: #fff;
    background-image: linear-gradient(to bottom, #FF857B, #FC003C);
}
