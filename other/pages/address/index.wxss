.addressPage {
    padding-bottom: 108rpx;    
}
/*地址列表*/
.addressItem {
    position: relative;
    z-index: 1;
    background-color: #fff;
    font-size: 28rpx;
    padding: 20rpx 24rpx;
    margin-bottom: 20rpx;
}
.phone {
    margin-left: 28rpx;    
}
.addressText {
    font-size: 24rpx;
    margin-top: 10rpx;
}
.addressItem-footer {
    display: flex;
    margin-top: 40rpx;
}
.addressItem-radio {
    flex: 0 0 250rpx;
    display: flex;
    align-items: center;    
}
.addressItem-footer-right {
    flex: 1 0 auto;
    display: flex;
    justify-content: flex-end;
}
.addressItem-operate {
    margin-left: 24rpx;    
}
.addressItem-operate-text {
    margin-left: 10rpx;
}
.currentAddress {
    position: absolute;
    z-index: 1;
    top: 20rpx;
    right: 20rpx;
    font-size: 32rpx;
    color: #fe3666;
}
/*地址列表end*/
/*底部按钮*/
.addressPage-footer{
    position: fixed;
    z-index: 2;
    right: 0;
    bottom: 0;
    left: 0;
    display: flex;
    padding: 20rpx 0;
    background-color: #f1f1f1;
}
.addressPage-footer-item {
    display: flex;
    flex: 1 1 auto;
    justify-content: center;
    align-items: center; 
}
.footerBotton {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center; 
    color: #fff;
    font-size: 32rpx;
    width: 300rpx;
    height: 88rpx;
    background-color: #FE3666;
    border-radius: 44rpx;
}
.footerBotton-text {
    margin-left: 10rpx;
} 
.wxBotton {
    background-color: #3EB135;
}
/*底部按钮end*/