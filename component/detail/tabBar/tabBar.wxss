@import "../../../style/iconfont.wxss";
/*------bottom-------*/
.live-wrapper{
  position: fixed;
  bottom: 110rpx;
  right: 20rpx;
  height: 100rpx;
  z-index:98 ;
  overflow: hidden;
  width: 60%;
}
.float{
  width: 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 100%;
  letter-spacing: 0;
  font-size: 28rpx;
  background: rgba(0, 0, 0, 0.74);
  opacity: 0;
  border-radius: 100rpx;
  box-shadow:0 0 2rpx 1rpx rgba(96, 96, 96, 0.4);
}
.float-content{
  display: flex;
  justify-content: left;
  align-items: center;
  width: 80%;
  height: 100%;
  padding: 0 10rpx;
}
.content-l{
  display: flex;
  align-items: center;
  position: relative;
}
.live-avatar{
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
}
.living{
  border-radius: 18rpx;
  width: 80%;
  margin-left: 10%;
  text-align: center;
  position: absolute;
  bottom: 0rpx;
  font-size: 18rpx;
  display: inline-block;
  background: #ff0000;
  color: #ffffff;
}
.content-r{
  width: 80%;
  margin-left: 20rpx;
}
.live-title{
  display: block;
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.live-info{
  display: flex;
  flex-direction: column;
  color: #ffffff;
}
.live-watcher{
  font-size: 20rpx;
}
.close{
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100%;
  width: 80rpx;
}
.icon-cuo{
  color: #ffffff;
  font-size: 26rpx;
}

.bottom{
  position: fixed;
  bottom: 0;
  height: 90rpx;
  width: 100%;
  background-color: #ffffff;
  display: flex;
  justify-content: space-between;
  align-items: center;
  z-index: 99;
}
.no-praise-price,.praise-price{
  display: flex;
  flex-direction: column;
  justify-content: center;
}
.praise-price-num{
  text-align: center;
}

.bottom-left{
  display: flex;
  justify-content: space-between;
}
.fff{
  color: #FFFFFF;
}
.bottom-item{
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  border-top:1rpx solid #eeeeee;
}
.share-box{
  background: #3EB135 !important;
  width: 1000rpx;
}
.share{
  background: transparent;
  font-size: 24rpx;
  display: flex;
  justify-content: center;
  padding: 0;
  color: #FFFFFF;
  flex-direction: row;
  line-height: initial;
}
.share::after{
  border: none;
}
.placeholder{
  height: 90rpx ;
  opacity: 0;
}
.home,.mymessage {
  width: 50%;
  text-align: center;
  display: flex;
  flex-direction: column;
  font-size: 20rpx;
  color: #888888;
  position: relative;
}
.chatTo {
  position: absolute;
  z-index: 1;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  opacity: 0;

}
.icon-home,.icon-mymessage{
  width: 40rpx;
  height: 40rpx;
  margin: 0 auto;
}

.icon-home::before,.icon-mymessage::before{
  font-size: 20px;
}
.no-praise-buy{
  background-color: #fff;
  color: #000;
}
.no-praise-buy:first-child{
  border-right: 1px solid #D8D8D8;
}
.nopraise{
  background-color: #FF0017;
}
.icon-weixin{
  margin-right: 10rpx;
}
.praise-buy{
  background-color: #FF0017;
  color: #ffffff;
}
.msg{
  position: absolute;
  top: -8rpx;

  left: 50%;
  min-width: 34rpx;
  background: #FE3666;
  font-size: 24rpx;
  color: #FFFFFF;
  letter-spacing: 0;
  border-radius: 50%;
}
/*------pop------*/
.order-img{
  width: 160rpx;
  height: 160rpx;
  background: #e9e9e9;
}
.header{
  display: flex;
  justify-content: left;
  align-items: flex-end;
  margin-top: -40rpx;
  padding: 0 0 20rpx;
}
.order-info{
  margin-left: 20rpx;
  width: 90%;
  overflow: hidden;
}
.order-price{
  color: #FF085E;
  font-size: 40rpx;
  letter-spacing: 0;
  line-height: 80rpx;
}
.order-attribute-box{
  padding: 0 10rpx;
  width: 100%;
  box-sizing: border-box;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.order-attribute{
  color: #FF085E;
}
.colors{

}
.form-header{
  color: #909090;
}
.form-content{
  display: flex;
  justify-content: left;
  align-items: center;
  margin-top: 10rpx;
  flex-direction: row;
  flex-wrap:wrap;
}
.form-content-item{
  padding: 10rpx 40rpx;
  margin-right:20rpx ;
  border: 2rpx solid rgba(74, 74, 74, 0.21);
  border-radius: 6rpx;
  margin-top: 20rpx;
  line-height: 46rpx;
  font-size: 24rpx;
}
.sizes .form-content-item{
  padding: 10rpx 60rpx;
}
.active{
  border: 1px solid #FE3666;
  color: #ffffff;
  background-color: #FE3666;
}
.none{
  background: #E8E8E8;
  border: 1px solid transparent;

  color: #CCCCCC;
  letter-spacing: 0;

}
.none-ok{
  padding: 20rpx;
  background: #E8E8E8;
  color: #CCCCCC;
  text-align: center;
}
.sizes ,.amount{
  margin-top: 20rpx;
}
.amount{
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 20rpx;
}

.nobutton{
  line-height: inherit;
  border: none;
  border-radius: 0;
  font-size: 24rpx;
  background: #fff;
  padding: 0;
}

.nobutton:after{
  border: none;
}
.pop-ok{
  padding: 20rpx;
  background-color: #FE3666;
  color: #ffffff;
  text-align: center;
}
.sold{
  width: 1000rpx;
  height: 100%;
  line-height:  90rpx;
  text-align: center;
  background:#909090;
  color: #ffffff;
}
.priceRange{
  display: flex;
  justify-content: left;
  align-items: center;
}
.priceRange-item{
  display: flex;
  flex-direction: column;
  margin-right: 20rpx;
}
.priceRange-item-num{
  font-size: 28rpx;
  color: #666666;
  letter-spacing: 0;
  line-height: 28rpx;
}
.scroll-view{
  max-height: 50vh;

}
.stock{
  color: #888888;
  text-align: right;
  margin-right: 20rpx;
  font-size: 28rpx;
}
.fixedNav {
  position: fixed;
  bottom: 200rpx;
  right: 20rpx;
  z-index: 11;

}
.fixedNavBtn{
  border-radius: 50%;
  width: 100rpx;
  height: 100rpx;
  background-color: #eee;
  margin-bottom: 20rpx;
  position: relative;
  text-align: center;
  box-shadow: 0 3px 8px #ccc;
}
.fixedNavBtn .iconfont{
  display: block;
  font-size: 60rpx;
}
.fixedNavBtn .pt{
  padding-top: 10rpx;
}
.fixedNavBtn-home{
  background-image: linear-gradient(-138deg, #fd9367 0%, #f02354 100%);
  color: #fff;
}
.fixedNavBtn-service{
  background: #fff;
  color: #18a413;
}
.home-text{
  display: block;
  margin-top: -8rpx;
  font-size: 10px;
}
.bottom-btn-home{
  margin-top: 0rpx;
  font-size: inherit;
}
.mymessage{
  text-align: center;
}